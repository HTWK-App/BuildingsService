# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
# Old API
GET     /building							controllers.v1.buildings
GET     /building/:id						controllers.v1.buildingDetails(id: String)
# NEW API
GET		/v2/buildings						controllers.v2.buildings
GET		/v2/building/:id					controllers.v2.buildingDetails(id: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file						controllers.Assets.at(path="/public", file)
